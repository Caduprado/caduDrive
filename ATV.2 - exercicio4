def sum(numero, Primo):
    return ()

numero = int(input('Digite um Numero entre 1 e 100: '))
Primo = 0


for i in range(1, (numero + 1)):        
  if numero % i == 0:
    Primo += 1
    
if Primo  == 2 :
  print('O número é primo')
else:
  print('O número não é primo')

